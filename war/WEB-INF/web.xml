<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<servlet>
		<servlet-name>SubmitCompanyServlet</servlet-name>
		<servlet-class>directoryControls.SubmitCompanyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SubmitCompanyServlet</servlet-name>
		<url-pattern>/SubmitCompanyServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RetrieveCompanyServlet</servlet-name>
		<servlet-class>directoryControls.RetrieveCompanyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RetrieveCompanyServlet</servlet-name>
		<url-pattern>/RetrieveCompanyServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SearchServlet</servlet-name>
		<servlet-class>directoryControls.SearchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchServlet</servlet-name>
		<url-pattern>/SearchServlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>